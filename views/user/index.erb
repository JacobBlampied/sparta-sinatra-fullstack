<h1>Index Page</h1>

<table class="table">
  <thead>
    <th>ID</th>
    <th>User Name</th>
    <th>Job</th>
    <th>Phone</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><a href="/users/<%= user.id %>"><%= user.name %></a></td>
        <td><%= user.job %></td>
        <td><%= user.phone %></td>

      </tr>
    <% end %>
  </tbody>
</table>
